{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "outputs": ["dist/**", "storybook-static/**"],
            "dependsOn": ["^build"]
        },
        "test": {
            "outputs": ["coverage/**"],
            "dependsOn": [],
            "cache": false
        },
        "lint": {},
        "clean": {
            "cache": false
        },
        "dev": {
            "cache": true,
            "persistent": false
        },
        "docs:dev": {},
        "docs:build": {},
        "docs:preview": {}
    }
}
